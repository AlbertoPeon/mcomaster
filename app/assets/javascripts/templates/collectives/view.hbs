<div class="box">
<h3>{{id}} collective</h3>
</div>
<p>This collective currently has <strong>{{collectiveSize}}</strong> members.</p>
<table class="table">
<thead>
<tr><th>Node</th></tr>
</thead>
<tbody>
	{{#each members}}
	<tr>
		<td><a href="/#/node/{{.}}">{{.}}</a></td>
	</tr>
	{{/each}}
</tbody>
</table>
