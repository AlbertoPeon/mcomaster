{{#if error}}
<div class="well" style="overflow:hidden;">
	<div style="float:left;padding-right:20px;clear:both;display:block;"><img src="/assets/fail48.png"/></div>
	<p style="font-weight:bold;font-size:20px;">Error</p>
	<p>{{error}}</p>
</div>
{{/if}}
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<td class="node-status-icon"></td>
			<td>Node</td>
			<td>Status</td>
			{{#each columns}}
				<td>{{display_as}}</td>
			{{/each}}
		</tr>
	</thead>
	<tbody>
	</tbody>
</table>