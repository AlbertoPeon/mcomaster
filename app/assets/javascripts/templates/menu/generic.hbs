<div class="accordion-heading">
	<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapse{{menuName}}">{{menuName}}</a>
</div>
<div id="collapse{{menuName}}" class="accordion-body collapse in">
	<ul aria-labelledby="dropdownMenu" role="menu" class="dropdown-menu"></ul>
	{{#if paginated}}
	<div class="menu-pagination">
		<div>
			<a href="#">Prev</a>
			<a href="#">1</a>
			<a href="#">2</a>
			<a href="#">3</a>
			<a href="#">4</a>
			<a href="#">5</a>
			<a href="#">Next</a>
		</div>
	</div>
	{{/if}}
</div>
